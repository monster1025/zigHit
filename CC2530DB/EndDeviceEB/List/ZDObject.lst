   2579                }
   2580                else
   2581                {
   2582                  status = ZDP_NO_MATCH;
   2583                  sendRsp = TRUE;
   2584                }
   2585            }
   2586          
   2587            if ( sendRsp )
   2588            {
   2589              // send response to this requester
   2590              dstAddr.addrMode = Addr16Bit;
   2591              dstAddr.addr.shortAddr = bindReq->srcAddr;
   2592              ZDP_EndDeviceBindRsp( bindReq->TransSeq, &dstAddr, status, bindReq->SecurityUse );
   2593          
   2594              if ( matchED->state == ZDMATCH_SENDING_BINDS )
   2595              {
   2596                // send response to first requester
   2597                dstAddr.addrMode = Addr16Bit;
   2598                dstAddr.addr.shortAddr = matchED->ed1.srcAddr;
   2599                ZDP_EndDeviceBindRsp( matchED->ed1.TransSeq, &dstAddr, status, matchED->ed1.SecurityUse );
   2600              }
