   2475           * @fn      ZDO_MatchEndDeviceBind()
   2476           *
   2477           * @brief
   2478           *
   2479           *   Called to match end device binding requests
   2480           *
   2481           * @param  bindReq  - binding request information
   2482           * @param  SecurityUse - Security enable/disable
   2483           *
   2484           * @return  none
   2485           */
   2486          void ZDO_MatchEndDeviceBind( ZDEndDeviceBind_t *bindReq )
   2487          {
   2488            zAddrType_t dstAddr;
   2489            uint8 sendRsp = FALSE;
   2490            uint8 status;
   2491          
   2492            // Is this the first request?
   2493            if ( matchED == NULL )
   2494            {
   2495              // Create match info structure
   2496              matchED = (ZDMatchEndDeviceBind_t *)osal_mem_alloc( sizeof ( ZDMatchEndDeviceBind_t ) );
   2497              if ( matchED )
   2498              {
   2499                // Clear the structure
   2500                osal_memset( (uint8 *)matchED, 0, sizeof ( ZDMatchEndDeviceBind_t ) );
   2501          
   2502                // Copy the first request's information
   2503                if ( !ZDO_CopyMatchInfo( &(matchED->ed1), bindReq ) )
   2504                {
   2505                  status = ZDP_NO_ENTRY;
   2506                  sendRsp = TRUE;
   2507                }
   2508              }
   2509              else
   2510              {
   2511                status = ZDP_NO_ENTRY;
   2512                sendRsp = TRUE;
   2513              }
   2514          
   2515              if ( !sendRsp )
   2516              {
   2517                // Set into the correct state
   2518                matchED->state = ZDMATCH_WAIT_REQ;
   2519          
   2520                // Setup the timeout
   2521                APS_SetEndDeviceBindTimeout( AIB_MaxBindingTime, ZDO_EndDeviceBindMatchTimeoutCB );
   2522              }
   2523            }
   2524            else
   2525            {
   2526                matchED->state = ZDMATCH_SENDING_BINDS;
   2527          
   2528                // Copy the 2nd request's information
   2529                if ( !ZDO_CopyMatchInfo( &(matchED->ed2), bindReq ) )
   2530                {
   2531                  status = ZDP_NO_ENTRY;
   2532                  sendRsp = TRUE;
   2533                }
   2534          
   2535                // Make a source match for ed1
   2536                matchED->ed1numMatched = ZDO_CompareClusterLists(
   2537                            matchED->ed1.numOutClusters, matchED->ed1.outClusters,
   2538                            matchED->ed2.numInClusters, matchED->ed2.inClusters, ZDOBuildBuf );
   2539                if ( matchED->ed1numMatched )
   2540                {
   2541                  // Save the match list
   2542                  matchED->ed1Matched = osal_mem_alloc( (short)(matchED->ed1numMatched * sizeof ( uint16 )) );
   2543                  if ( matchED->ed1Matched )
   2544                  {
   2545                    osal_memcpy( matchED->ed1Matched, ZDOBuildBuf, (matchED->ed1numMatched * sizeof ( uint16 )) );
   2546                  }
   2547                  else
   2548                  {
   2549                    // Allocation error, stop
   2550                    status = ZDP_NO_ENTRY;
   2551                    sendRsp = TRUE;
   2552                  }
   2553                }
   2554          
   2555                // Make a source match for ed2
   2556                matchED->ed2numMatched = ZDO_CompareClusterLists(
   2557                            matchED->ed2.numOutClusters, matchED->ed2.outClusters,
   2558                            matchED->ed1.numInClusters, matchED->ed1.inClusters, ZDOBuildBuf );
   2559                if ( matchED->ed2numMatched )
   2560                {
   2561                  // Save the match list
   2562                  matchED->ed2Matched = osal_mem_alloc( (short)(matchED->ed2numMatched * sizeof ( uint16 )) );
   2563                  if ( matchED->ed2Matched )
   2564                  {
   2565                    osal_memcpy( matchED->ed2Matched, ZDOBuildBuf, (matchED->ed2numMatched * sizeof ( uint16 )) );
   2566                  }
   2567                  else
   2568                  {
   2569                    // Allocation error, stop
   2570                    status = ZDP_NO_ENTRY;
   2571                    sendRsp = TRUE;
   2572                  }
   2573                }
   2574          
   2575                if ( (sendRsp == FALSE) && (matchED->ed1numMatched || matchED->ed2numMatched) )
   2576                {
   2577                  // Do the first unbind/bind state
