   2627                osal_mem_free( matchED->ed1Matched );
   2628              }
   2629              if ( matchED->ed1.inClusters != NULL )
   2630              {
   2631                osal_mem_free( matchED->ed1.inClusters );
   2632              }
   2633              if ( matchED->ed1.outClusters != NULL )
   2634              {
   2635                osal_mem_free( matchED->ed1.outClusters );
   2636              }
   2637              if ( matchED->ed2.inClusters != NULL )
   2638              {
   2639                osal_mem_free( matchED->ed2.inClusters );
   2640              }
   2641              if ( matchED->ed2.outClusters != NULL )
   2642              {
   2643                osal_mem_free( matchED->ed2.outClusters );
   2644              }
   2645          
   2646              osal_mem_free( matchED );
