   2719          /*********************************************************************
   2720           * @fn      ZDMatchSendState()
   2721           *
   2722           * @brief   State machine for the End device match message algorithm.
   2723           *
   2724           * @param  reason - state of algoritm
   2725           * @param  status - initial message status
   2726           * @param  TransSeq - next transaction sequence number
   2727           *
   2728           * @return  FALSE if error and we are not currently matching, TRUE
   2729           *          if success.
   2730           */
   2731          uint8 ZDMatchSendState( uint8 reason, uint8 status, uint8 TransSeq )
   2732          {
   2733            uint8 *dstIEEEAddr = NULL;
   2734            uint8 dstEP = 0xFF;
   2735            zAddrType_t dstAddr;
   2736            zAddrType_t destinationAddr;
   2737            uint16 msgType;
   2738            uint16 clusterID = 0xFFFF;
   2739            ZDEndDeviceBind_t *ed = NULL;
   2740            uint8 rspStatus = ZDP_SUCCESS;
   2741          
   2742            if ( matchED == NULL )
   2743            {
   2744              return ( FALSE );
   2745            }
   2746          
   2747            // Check sequence number
   2748            if ( reason == ZDMATCH_REASON_BIND_RSP || reason == ZDMATCH_REASON_UNBIND_RSP )
   2749            {
   2750              if ( TransSeq != matchED->transSeq )
   2751              {
   2752                return( FALSE ); // ignore the message
   2753              }
   2754            }
   2755          
   2756            // turn off timer
   2757            APS_SetEndDeviceBindTimeout( 0, ZDO_EndDeviceBindMatchTimeoutCB );
   2758          
   2759            if ( reason == ZDMATCH_REASON_TIMEOUT )
   2760            {
   2761              rspStatus = ZDP_TIMEOUT;    // The process will stop
   2762            }
   2763          
   2764            if ( reason == ZDMATCH_REASON_START || reason == ZDMATCH_REASON_BIND_RSP )
   2765            {
   2766              matchED->sending = ZDMATCH_SENDING_UNBIND;
   2767          
   2768              if ( reason == ZDMATCH_REASON_BIND_RSP && status != ZDP_SUCCESS )
   2769              {
   2770                rspStatus = status;
   2771              }
   2772            }
   2773            else if ( reason == ZDMATCH_REASON_UNBIND_RSP )
   2774            {
   2775              if ( status == ZDP_SUCCESS )
   2776              {
   2777                matchED->sending = ZDMATCH_SENDING_UNBIND;
   2778              }
   2779              else
   2780              {
   2781                matchED->sending = ZDMATCH_SENDING_BIND;
   2782              }
   2783            }
   2784          
   2785            if ( reason != ZDMATCH_REASON_START && matchED->sending == ZDMATCH_SENDING_UNBIND )
   2786            {
   2787              // Move to the next cluster ID
   2788              if ( matchED->ed1numMatched )
   2789              {
   2790                matchED->ed1numMatched--;
   2791              }
   2792              else if ( matchED->ed2numMatched )
   2793              {
   2794                matchED->ed2numMatched--;
   2795              }
   2796            }
   2797          
   2798            // What message do we send now
   2799            if ( matchED->ed1numMatched )
   2800            {
   2801              ed = &(matchED->ed1);
   2802              clusterID = matchED->ed1Matched[matchED->ed1numMatched-1];
   2803              dstIEEEAddr = matchED->ed2.ieeeAddr;
   2804              dstEP = matchED->ed2.endpoint;
   2805            }
   2806            else if ( matchED->ed2numMatched )
   2807            {
   2808              ed = &(matchED->ed2);
   2809              clusterID = matchED->ed2Matched[matchED->ed2numMatched-1];
   2810              dstIEEEAddr = matchED->ed1.ieeeAddr;
   2811              dstEP = matchED->ed1.endpoint;
   2812            }
   2813          
   2814            dstAddr.addrMode = Addr16Bit;
   2815          
   2816            // Send the next message
   2817            if ( (rspStatus == ZDP_SUCCESS) && ed )
   2818            {
   2819              // Send unbind/bind message to source
   2820              if ( matchED->sending == ZDMATCH_SENDING_UNBIND )
   2821              {
   2822                msgType = Unbind_req;
   2823              }
   2824              else
   2825              {
   2826                msgType = Bind_req;
   2827              }
   2828          
   2829              dstAddr.addr.shortAddr = ed->srcAddr;
   2830          
   2831              // Save off the transaction sequence number
   2832              matchED->transSeq = ZDP_TransID;
   2833          
   2834              destinationAddr.addrMode = Addr64Bit;
   2835              osal_cpyExtAddr( destinationAddr.addr.extAddr, dstIEEEAddr );
   2836          
   2837              ZDP_BindUnbindReq( msgType, &dstAddr, ed->ieeeAddr, ed->endpoint, clusterID,
   2838                  &destinationAddr, dstEP, ed->SecurityUse );
   2839          
   2840              // Set timeout for response
   2841              APS_SetEndDeviceBindTimeout( AIB_MaxBindingTime, ZDO_EndDeviceBindMatchTimeoutCB );
   2842            }
   2843            else
   2844            {
   2845              // Send the response messages to requesting devices
   2846              // send response to first requester
   2847              dstAddr.addr.shortAddr = matchED->ed1.srcAddr;
   2848              ZDP_EndDeviceBindRsp( matchED->ed1.TransSeq, &dstAddr, rspStatus, matchED->ed1.SecurityUse );
   2849          
   2850              // send response to second requester
   2851              if ( matchED->state == ZDMATCH_SENDING_BINDS )
