   2661          static uint8 ZDO_CopyMatchInfo( ZDEndDeviceBind_t *destReq, ZDEndDeviceBind_t *srcReq )
   2662          {
   2663            uint8 allOK = TRUE;
   2664          
   2665            // Copy bind information into the match info structure
   2666            osal_memcpy( (uint8 *)destReq, srcReq, sizeof ( ZDEndDeviceBind_t ) );
   2667          
   2668            // Initialize the destination cluster pointers
   2669            destReq->inClusters = NULL;
   2670            destReq->outClusters = NULL;
   2671          
   2672            // Copy input cluster IDs
   2673            if ( srcReq->numInClusters )
   2674            {
   2675              destReq->inClusters = osal_mem_alloc( (short)(srcReq->numInClusters * sizeof ( uint16 )) );
   2676              if ( destReq->inClusters )
   2677              {
   2678                // Copy the clusters
   2679                osal_memcpy( (uint8*)(destReq->inClusters), (uint8 *)(srcReq->inClusters),
   2680                                (srcReq->numInClusters * sizeof ( uint16 )) );
   2681              }
   2682              else
   2683              {
   2684                allOK = FALSE;
   2685              }
   2686            }
   2687          
   2688            // Copy output cluster IDs
   2689            if ( srcReq->numOutClusters )
   2690            {
   2691              destReq->outClusters = osal_mem_alloc( (short)(srcReq->numOutClusters * sizeof ( uint16 )) );
   2692              if ( destReq->outClusters )
   2693              {
   2694                // Copy the clusters
   2695                osal_memcpy( (uint8 *)(destReq->outClusters), (uint8 *)(srcReq->outClusters),
   2696                                (srcReq->numOutClusters * sizeof ( uint16 )) );
   2697              }
   2698              else
   2699              {
   2700                allOK = FALSE;
   2701              }
   2702            }
   2703          
   2704            if ( allOK == FALSE )
   2705            {
   2706              if ( destReq->inClusters != NULL )
   2707              {
   2708                osal_mem_free( destReq->inClusters );
   2709              }
   2710              if ( destReq->outClusters != NULL )
   2711              {
   2712                osal_mem_free( destReq->outClusters );
   2713              }
   2714            }
   2715          
   2716            return ( allOK );
   2717          }
